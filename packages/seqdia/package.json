{
  "name": "seqdia",
  "version": "0.2.0",
  "description": "React components for interactive sequence diagrams with hierarchical actor grouping",
  "author": "Christian Legnitto",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "vitest run",
    "prepublishOnly": "pnpm test && pnpm build"
  },
  "keywords": [
    "react",
    "sequence-diagram",
    "diagram",
    "visualization",
    "typescript",
    "tailwind",
    "uml",
    "flowchart"
  ],
  "license": "MIT",
  "homepage": "https://github.com/LegNeato/seqdia#readme",
  "bugs": {
    "url": "https://github.com/LegNeato/seqdia/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LegNeato/seqdia.git"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.2"
  }
}
